    "request-promise": "^4.2.6",
    "split": "^1.0.1",
    "telnet-client": "^1.4.11",
    "typed-error": "^3.2.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@balena/lint": "^9.3.12",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.7",
