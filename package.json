  "engines": {
    "node": "^20.6.1",
    "npm": "^10.1.0"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint -t ./tsconfig.dev.json --fix"
    ]
  },
  "mocha": {
    "bail": true,
    "reporter": "spec",
    "require": "ts-node/register/transpile-only",
    "exit": true
