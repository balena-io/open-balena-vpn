{
  "name": "open-balena-vpn",
  "private": true,
  "version": "9.17.19",
  "description": "RESTful VPN api",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "tsc --project .",
    "check": "tsc --pretty --noEmit --diagnostics --project ./tsconfig.json",
    "check:dev": "tsc --pretty --noEmit --diagnostics --project ./tsconfig.dev.json",
    "lint": "balena-lint src/ test/ typings/",
    "lint:fix": "balena-lint --fix src/ test/ typings/",
    "pretest-unit": "npm run lint",
    "test-unit": "mocha test/index.ts",
    "test": "docker build -t test-balena-vpn . && IMAGE_NAME=test-balena-vpn ./automation/test.sh"
  },
  "dependencies": {
    "@balena/node-metrics-gatherer": "^5.7.5",
    "@sentry/node": "^5.30.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "event-stream": "^4.0.1",
    "eventemitter3": "^4.0.7",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "morgan": "^1.10.0",
    "netmask": "^2.0.2",
    "node-tunnel": "^4.0.1",
    "pinejs-client-request": "^7.3.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "source-map-support": "^0.5.19",
    "telnet-openvpn": "^0.1.9",
    "typed-error": "^3.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/compression": "^1.7.1",
    "@types/event-stream": "^3.3.34",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/memoizee": "^0.4.6",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/netmask": "^1.0.30",
    "@types/node": "^14.17.9",
    "@types/request-promise": "^4.1.48",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "mocha": "^9.0.3",
    "nock": "^13.1.1",
    "openvpn-client": "0.0.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "mocha": {
    "bail": true,
    "reporter": "spec",
    "require": "ts-node/register/transpile-only",
    "exit": true
  },
  "versionist": {
    "publishedAt": "2021-08-19T13:04:50.302Z"
  }
}
