    "@types/chai": "^5.2.3",
    "@types/chai-as-promised": "^8.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.25",
    "@types/lodash": "^4.17.21",
    "@types/memoizee": "^0.4.12",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "@types/netmask": "^2.0.6",
    "@types/node": "^24.10.4",
    "@types/request-promise": "^4.1.51",
    "@types/split": "^1.0.5",
    "balena-sdk": "^22.4.4",
    "chai": "^6.2.2",
    "chai-as-promised": "^8.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "nock": "^13.5.6",
    "openvpn-client": "0.0.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^24.5.0",
    "npm": "^11.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint -t ./tsconfig.dev.json --fix"
    ]
  },
