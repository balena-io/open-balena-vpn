  "engines": {
    "node": "^22.2.0",
    "npm": "^10.7.0"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint -t ./tsconfig.dev.json --fix"
    ]
  },
  "mocha": {
    "bail": true,
    "reporter": "spec",
    "loader": "ts-node/esm/transpile-only",
    "require": "ts-node/register/transpile-only",
    "exit": true
  },
