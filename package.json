    "@opentelemetry/instrumentation-http": "0.211.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@sentry/node": "^10.32.1",
    "bluebird": "^3.7.2",
    "body-parser": "^2.2.1",
    "compression": "^1.8.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.22.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.17",
    "morgan": "^1.10.1",
    "netmask": "^2.0.2",
