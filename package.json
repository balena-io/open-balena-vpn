    "@types/event-stream": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.198",
    "@types/memoizee": "^0.4.8",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.5",
    "@types/netmask": "^2.0.2",
    "@types/node": "^20.6.0",
    "@types/request-promise": "^4.1.48",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "husky": "^8.0.3",
