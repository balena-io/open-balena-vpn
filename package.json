  "devDependencies": {
    "@balena/lint": "^9.0.1",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^5.0.0",
    "@types/chai-as-promised": "^8.0.0",
    "@types/compression": "^1.7.5",
    "@types/event-stream": "^4.0.5",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/memoizee": "^0.4.11",
    "@types/mocha": "^10.0.8",
    "@types/morgan": "^1.9.9",
    "@types/netmask": "^2.0.5",
    "@types/node": "^22.5.5",
    "@types/request-promise": "^4.1.51",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "husky": "^9.1.6",
